<link rel="stylesheet" href="/css/cadastrar_adm.css">

<h1>Cadastrar um Administrador</h1>
<br>

<form action="/cadastrar_adm" method="post">
    <input type="text" name="nome" placeholder="Digite seu Nome"><br><br>
    <input type="text" name="sobrenome" placeholder="Digite seu Sobrenome"><br><br>
    <input type="email" name="email" placeholder="Digite seu email"><br><br>
    <input type="text" name="telefone" placeholder="Digite seu telefone"><br><br>
    <input type="text" name="cpf" placeholder="Digite seu cpf"><br><br>
    <input type="password" name="senha" placeholder="Digite sua senha"><br>
    <br>
    <button type="submit">Cadastrar</button>
    {{#if msg}}
    <p>{{msg}}</p>
    {{/if}}
    </form>